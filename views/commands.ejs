<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Features</title>
</head>
<body>
    <h1>Commands</h1>
    <% commands.each(({ value }) => { %>
        <table style="width: 100%;">
            <tr>
                <th>Command Name</th> 
            </tr>
            <% value.each(({ name }) => { %>
                <tr>
                    <td>
                        <%= name%>
                    </td>
                </tr>
            <% }) %>
        </table>
    <% }) %>
</body>
</html>